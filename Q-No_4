def can_place_flowers(flowerbed, n):
    count = 0
    length = len(flowerbed)
    i = 0
    
    while i < length:
        if flowerbed[i] == 0 and (i == 0 or flowerbed[i-1] == 0) and (i == length-1 or flowerbed[i+1] == 0):
            flowerbed[i] = 1
            count += 1
            i += 2
        else:
            i += 1
    
    return count >= n

flowerbed = [1, 0, 0, 0, 1]
n = 1
print(can_place_flowers(flowerbed, n))
